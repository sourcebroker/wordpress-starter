{
  "name": "sourcebroker/wordpress-starter",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate installed in root with bedrock style configuration (.env / environments)",
  "config": {
    "preferred-install": "dist",
    "process-timeout": 99999,
    "platform": {
      "php": "7.3"
    }
  },
  "require": {
    "symfony/dotenv": "^4.0 || ^5.0",
    "oscarotero/env": "^1.2",
    "roots/wp-config": "^1.0.0",
    "roots/wp-password-bcrypt": "^1.0",
    "sourcebroker/deployer-extended-wordpress": "dev-master",
    "wp-cli/core-command": "^2.0",
    "wp-cli/db-command": "^2.0",
    "wp-cli/rewrite-command": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
